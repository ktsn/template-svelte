{
  "name": "template-svelte",
  "version": "0.0.0",
  "description": "Template for Svelte component",
  "keywords": [
    "template",
    "component",
    "UI",
    "Svelte"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/template-svelte.cjs.js",
  "module": "dist/template-svelte.es.js",
  "unpkg": "dist/template-svelte.js",
  "author": "katashin",
  "license": "MIT",
  "homepage": "https://github.com/ktsn/template-svelte",
  "bugs": "https://github.com/ktsn/template-svelte/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/template-svelte.git"
  },
  "scripts": {
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "build": "bili --format cjs,es,umd,umd-min",
    "lint": "eslint --ext html src test",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "transform": {
      "\\.js$": "babel-jest",
      "\\.html$": "svelte-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "html"
    ],
    "testRegex": "/test/.+\\.spec\\.js$"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.0.0-alpha.0",
    "bili": "^3.0.11",
    "eslint": "^4.19.0",
    "eslint-config-ktsn": "^1.0.3",
    "eslint-plugin-html": "^4.0.2",
    "jest": "^22.4.2",
    "npm-run-all": "^4.1.2",
    "postcss": "^6.0.21",
    "postcss-custom-properties": "^7.0.0",
    "prettier": "1.11.1",
    "rollup-plugin-svelte": "^4.0.0",
    "svelte": "^1.58.0",
    "svelte-jest": "^0.1.0"
  }
}
